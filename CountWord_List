package ListArr;

public class ArrayCount_Word {

	public static void main(String[] args){
		
		int c=0;
		String arr[]={"String","Story","dancing","writing","poet","Scripting","Distance"};
		
		for(int i=0;i<arr.length;i++) {
			if(arr[i].endsWith("ing"))
			{
				c++;
			}
		
		}
		System.out.println(c);		
		}
}
